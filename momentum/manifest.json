{
  "manifest_version": 2,
  "name": "Momentum",
  "version": "1.0",
  "description": "Momentum Task by Liliya Voloshina",
  "icons": {
    "32": "favicon.ico"
  },
  "browser_action": {
    "default_icon": "./img/favicon.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "permissions": ["activeTab", "storage", "clipboardWrite"]
}
